31865057611__mold_calculator_h_qH63id');\n\n      var FT_PER_M = 3.28084;\n      var MIN_M = 0.5;\n      var MAX_M = 10;\n      var currentUnit = 'm';\n\n      /* ===== PRICING ===== */\n      var UNIT_COMPARE = 128.70;  /* prix barr√© par unit√© */\n      var UNIT_PRICE   = 69.97;   /* prix Pack Maison par unit√© */\n\n      var packs = [\n        { name: 'Essentiel',    qty: 1, price: 79.90,  compare: 128.70, save: 48,  badge: 'eco',  badgeText: 'D√©couverte',      desc: '1 zone cibl√©e' },\n        { name: 'Pack Duo ‚≠ê',  qty: 2, price: 149.90, compare: 248.40, save: 98,  badge: 'best', badgeText: 'N¬∞1 des ventes',  desc: 'Pi√®ce enti√®re' },\n        { name: 'Pack Maison',  qty: 3, price: 209.90, compare: 348.30, save: 138, badge: 'max',  badgeText: 'Max. √©conomies',  desc: 'Multi-pi√®ces' }\n      ];\n\n      function toFt(m){ return m * FT_PER_M; }\n      function toM(ft){ return ft / FT_PER_M; }\n      function oneDec(x){ return (Math.round(x*10)/10).toFixed(1); }\n      function fmtPrice(p){ return p.toFixed(2).replace('.', ',') + ' ‚Ç¨'; }\n\n      /* Arrondir au ,90 sup√©rieur */\n      function roundTo90(p){\n        var base = Math.floor(p);\n        return base + 0.90;\n      }\n\n      function syncSliderBounds(areaMeters){\n        if(currentUnit === 'm'){\n          range.min = MIN_M; range.max = MAX_M; range.step = 0.5;\n          range.value = oneDec(areaMeters);\n          minLb.textContent = oneDec(MIN_M) + ' m';\n          maxLb.textContent = oneDec(MAX_M) + ' m';\n        } else {\n          var minFt = toFt(MIN_M), maxFt = toFt(MAX_M);\n          range.min = oneDec(minFt); range.max = oneDec(maxFt); range.step = 0.5;\n          range.value = oneDec(toFt(areaMeters));\n          minLb.textContent = oneDec(minFt) + ' ft';\n          maxLb.textContent = oneDec(maxFt) + ' ft';\n        }\n      }\n\n      function getAreaMetersFromSlider(){\n        var v = parseFloat(range.value);\n        return currentUnit === 'm' ? v : toM(v);\n      }\n\n      function updateToggleUI(){\n        unitLb.textContent = currentUnit;\n        var buttons = unitTg.querySelectorAll('button');\n        buttons.forEach(function(btn){\n          if(btn.dataset.unit === currentUnit) btn.classList.add('is-active');\n          else btn.classList.remove('is-active');\n        });\n      }\n\n      /* ===== PACK RECOMMENDATION ===== */\n      function updatePackReco(modules){\n        if(modules <= 0) modules = 1;\n\n        if(modules <= 3){\n          /* Tiers 1-3 : recommandation directe */\n          var pack = packs[modules - 1];\n          packBadge.innerHTML = '<span class=\"pack-reco-badge pack-reco-badge--' + pack.badge + '\">' + pack.badgeText + '</span>';\n          packName.textContent = pack.name;\n          packDetail.textContent = pack.desc + ' ¬∑ Couvre jusqu\\'√† ' + (modules * 1.5).toFixed(1).replace('.0','') + ' m';\n          packPrice.innerHTML =\n            '<span class=\"pr-now\">' + fmtPrice(pack.price) + '</span>' +\n            '<span class=\"pr-was\">' + fmtPrice(pack.compare) + '</span>' +\n            '<span class=\"pr-save\">√âconomisez ' + pack.save + ' ‚Ç¨</span>';\n          packMulti.style.display = 'none';\n\n        } else {\n          /* 4+ modules : Pack Maison + unit√©s suppl√©mentaires */\n          var extra = modules - 3;\n          var rawPrice = 209.90 + (extra * UNIT_PRICE);\n          var price = roundTo90(rawPrice);\n          var compare = 348.30 + (extra * UNIT_COMPARE);\n          var save = Math.floor(compare - price);\n\n          packBadge.innerHTML = '<span class=\"pack-reco-badge pack-reco-badge--max\">Meilleure offre</span>';\n          packName.textContent = 'Pack Maison + ' + extra + ' panneau' + (extra > 1 ? 'x' : '');\n          packDetail.textContent = modules + ' panneaux ¬∑ Couvre jusqu\\'√† ' + (modules * 1.5).toFixed(1).replace('.0','') + ' m ¬∑ ' + fmtPrice(price / modules) + '/panneau';\n          packPrice.innerHTML =\n            '<span class=\"pr-now\">' + fmtPrice(price) + '</span>' +\n            '<span class=\"pr-was\">' + fmtPrice(compare) + '</span>' +\n            '<span class=\"pr-save\">√âconomisez ' + save + ' ‚Ç¨</span>';\n\n          packMulti.innerHTML =\n            'üëâ S√©lectionnez <strong>Pack Maison</strong> puis ajoutez <strong>' + extra + ' unit√©' + (extra > 1 ? 's' : '') + '</strong> suppl√©mentaire' + (extra > 1 ? 's' : '');\n          packMulti.style.display = 'block';\n        }\n      }\n\n      function calc(){\n        var areaMeters = getAreaMetersFromSlider();\n        var display = currentUnit === 'm' ? areaMeters : toFt(areaMeters);\n        areaV.textContent = oneDec(display);\n        unitLb.textContent = currentUnit;\n\n        var opt = sevSel.selectedOptions[0];\n        var factor = parseFloat(opt ? opt.dataset.factor : '1');\n        var op = (opt ? opt.dataset.op : 'multiply').toLowerCase();\n\n        var result = (op === 'divide') ? (areaMeters / (factor || 1)) : (areaMeters * (factor || 1));\n        result = Math.ceil(result);\n        if(result < 1) result = 1;\n\n        total.textContent = result;\n\n        var min = parseFloat(range.min), max = parseFloat(range.max);\n        var pct = ((parseFloat(range.value) - min) / (max - min)) * 100;\n        range.style.setProperty('--fill', pct + '%');\n\n        updatePackReco(result);\n      }\n\n      range.addEventListener('input', calc);\n      sevSel.addEventListener('change', calc);\n      unitTg.addEventListener('click', function(e){\n        var btn = e.target.closest('button[data-unit]');\n        if(!btn) return;\n        var oldMeters = getAreaMetersFromSlider();\n        currentUnit = btn.dataset.unit;\n        updateToggleUI();\n        syncSliderBounds(oldMeters);\n        calc();\n      });\n\n      updateToggleUI();\n      syncSliderBounds(3);\n      calc();\n    })();\n\n    function scrollToMoisissureBundles() {\n      var calcSection = document.getElementById('calc-template--25831865057611__mold_calculator_h_qH63id');\n      var target = null;\n\n      var allElements = document.querySelectorAll('*');\n      for (var i = 0; i < allElements.length; i++) {\n        var el = allElements[i];\n        var cls = (el.className && typeof el.className === 'string') ? el.className.toLowerCase() : '';\n        var eid = (el.id || '').toLowerCase();\n        if (cls.indexOf('kaching') > -1 || eid.indexOf('kaching') > -1 ||\n            cls.indexOf('bundle') > -1 || eid.indexOf('bundle') > -1 ||\n            cls.indexOf('monbundle') > -1 || eid.indexOf('monbundle') > -1) {\n          if (calcSection && (el === calcSection || calcSection.contains(el))) continue;\n          if (el.offsetHeight < 50) continue;\n          target = el; break;\n        }\n      }\n\n      if (!target) {\n        var formSelectors = [\n          'form[action*=\"/cart/add\"]', 'form[action*=\"/cart\"]',\n          '.product-form', '.product-form__buttons',\n          '[data-product-form]', '.shopify-product-form',\n          '#AddToCartForm', '.product__info-container', '.product__info-wrapper'\n        ];\n        for (var j = 0; j < formSelectors.length; j++) {\n          var found = document.querySelector(formSelectors[j]);\n          if (found && found.offsetHeight > 0) {\n            if (!calcSection || found.getBoundingClientRect().top < calcSection.getBoundingClientRect().top) {\n              target = found; break;\n            }\n          }\n        }\n      }\n\n      if (!target) {\n        var allButtons = document.querySelectorAll('button, .btn, a.button, [type=\"submit\"]');\n        for (var k = 0; k < allButtons.length; k++) {\n          var txt = (allButtons[k].textContent || '').trim().toLowerCase();\n          if (txt.indexOf('commander') > -1 || txt.indexOf('ajouter') > -1 || txt.indexOf('add to cart') > -1) {\n            if (calcSection && calcSection.contains(allButtons[k])) continue;\n            target = allButtons[k]; break;\n          }\n        }\n      }\n\n      if (target) {\n        var offsetTop = target.getBoundingClientRect().top + window.pageYOffset - 100;\n        window.scrollTo({ top: Math.max(0, offsetTop), behavior: 'smooth' });\n      }\n    }\n  </script>\n</section>",
