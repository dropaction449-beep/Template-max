  box-shadow: 0 2px 8px rgba(0,0,0,0.15);\n  transition: all 0.3s ease;\n}\n\n.video-btn-prev-template--24940377080074__product_video_mMfQVM {\n  left: 0;\n  transform: translateY(-50%) rotate(180deg);\n}\n\n.video-btn-next-template--24940377080074__product_video_mMfQVM {\n  right: 0;\n}\n\n.video-btn-prev-template--24940377080074__product_video_mMfQVM:hover,\n.video-btn-next-template--24940377080074__product_video_mMfQVM:hover {\n  background: #f0f0f0;\n  transform: translateY(-50%) scale(1.1);\n}\n\n.video-btn-prev-template--24940377080074__product_video_mMfQVM:hover {\n  transform: translateY(-50%) rotate(180deg) scale(1.1);\n}\n\n.video-btn-prev-template--24940377080074__product_video_mMfQVM svg,\n.video-btn-next-template--24940377080074__product_video_mMfQVM svg {\n  width: 20px;\n  height: 20px;\n}\n\n/* RESPONSIVE TABLET */\n@media (max-width: 1024px) {\n  .video-slide-template--24940377080074__product_video_mMfQVM {\n    flex: 0 0 calc(50% - 8px) !important;\n    width: calc(50% - 8px) !important;\n  }\n}\n\n/* RESPONSIVE MOBILE */\n@media (max-width: 768px) {\n  .video-slider-template--24940377080074__product_video_mMfQVM {\n    padding: 0 20px;\n  }\n  \n  .video-slide-template--24940377080074__product_video_mMfQVM {\n    flex: 0 0 100% !important;\n    width: 100% !important;\n  }\n  \n  .video-heading-template--24940377080074__product_video_mMfQVM h1 {\n    font-size: 24px;\n    margin-bottom: 30px;\n  }\n  \n  .video-btn-prev-template--24940377080074__product_video_mMfQVM,\n  .video-btn-next-template--24940377080074__product_video_mMfQVM {\n    width: 35px;\n    height: 35px;\n  }\n}\n</style>\n\n<!-- ============================================ -->\n<!-- JAVASCRIPT POUR SWIPER -->\n<!-- ============================================ -->\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n  // Attendre que Swiper soit chargé\n  function initVideoSwiper() {\n    if (typeof Swiper === 'undefined') {\n      setTimeout(initVideoSwiper, 100);\n      return;\n    }\n\n    const swiperElement = document.querySelector('.video-slider-template--24940377080074__product_video_mMfQVM');\n    \n    if (!swiperElement) return;\n\n    // Détruire l'instance existante si elle existe\n    if (swiperElement.swiper) {\n      swiperElement.swiper.destroy(true, true);\n    }\n\n    // Créer nouvelle instance\n    const videoSwiper = new Swiper('.video-slider-template--24940377080074__product_video_mMfQVM', {\n      slidesPerView: 3,\n      spaceBetween: 16,\n      navigation: {\n        nextEl: '.video-btn-next-template--24940377080074__product_video_mMfQVM',\n        prevEl: '.video-btn-prev-template--24940377080074__product_video_mMfQVM',\n      },\n      breakpoints: {\n        320: {\n          slidesPerView: 1,\n          spaceBetween: 10\n        },\n        768: {\n          slidesPerView: 2,\n          spaceBetween: 16\n        },\n        1024: {\n          slidesPerView: 3,\n          spaceBetween: 16\n        }\n      },\n      loop: false,\n      watchOverflow: true,\n    });\n\n    // Forcer le chargement des vidéos\n    setTimeout(function() {\n      document.querySelectorAll('.video-slider-template--24940377080074__product_video_mMfQVM video').forEach(function(video) {\n        video.load();\n      });\n    }, 100);\n  }\n\n  initVideoSwiper();\n});\n</script>"
