= track.querySelectorAll('.reviews-slide');\n      const totalSlides = slides.length;\n\n      function getSlidesPerView() {\n        return window.innerWidth >= 1000 ? 3 : 1;\n      }\n\n      let currentIndex = 0;\n      let slidesPerView = getSlidesPerView();\n      let totalPages = Math.ceil(totalSlides / slidesPerView);\n      let dots = [];\n\n      // Cr√©er un dot par \"page\"\n      function createDots() {\n        dotsContainer.innerHTML = '';\n        dots = [];\n        totalPages = Math.ceil(totalSlides / slidesPerView);\n        for (let i = 0; i < totalPages; i++) {\n          const dotBtn = document.createElement('button');\n          dotBtn.addEventListener('click', () => {\n            goToSlide(i * slidesPerView);\n          });\n          dotsContainer.appendChild(dotBtn);\n          dots.push(dotBtn);\n        }\n      }\n\n      function updateSlider() {\n        slidesPerView = getSlidesPerView();\n        createDots();\n\n        if (currentIndex >= totalSlides) {\n          currentIndex = totalSlides - slidesPerView;\n        }\n        if (currentIndex < 0) currentIndex = 0;\n\n        const slideWidth = track.querySelector('.reviews-slide').clientWidth;\n        const offset = slideWidth * currentIndex;\n        track.style.transform = `translateX(-${offset}px)`;\n\n        // Activer le dot correspondant\n        dots.forEach((dotBtn, idx) => {\n          dotBtn.classList.remove('is-active');\n          if (idx === Math.floor(currentIndex / slidesPerView)) {\n            dotBtn.classList.add('is-active');\n          }\n        });\n      }\n\n      function goToSlide(index) {\n        currentIndex = index;\n        updateSlider();\n      }\n\n      btnPrev.addEventListener('click', () => {\n        currentIndex -= slidesPerView;\n        updateSlider();\n      });\n      btnNext.addEventListener('click', () => {\n        currentIndex += slidesPerView;\n        updateSlider();\n      });\n      window.addEventListener('resize', () => {\n        updateSlider();\n      });\n\n      // Init\n      updateSlider();\n    })();\n  </script>\n\n  \n</section>\n</div>",
