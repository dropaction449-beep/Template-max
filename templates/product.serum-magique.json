ottom:6px;\n        padding-left:8px; padding-right:8px;\n        margin-left:-8px; margin-right:-8px;\n        scroll-padding:16px;\n      }\n      .leaf-ingredients__track::-webkit-scrollbar{display:none;}\n    }\n\n    .leaf-ingredients__card{\n      background-image: var(--card-bg);\n      background-size: cover; background-position: 50% 50%;\n      border-radius: var(--radius);\n      position: relative; min-height: 520px;\n      display:flex; align-items:flex-end; justify-content:center;\n      scroll-snap-align: center;\n      box-shadow: none;\n      filter: drop-shadow(0px 0px 57px rgba(125,118,118,0.27));\n    }\n    @media (min-width: 990px){ .leaf-ingredients__card{min-height: 540px;} }\n    @media (max-width: 749px){\n      .leaf-ingredients__card{\n        min-height: 520px;\n        flex: 0 0 90%;\n        filter: none !important;\n        box-shadow: none !important;\n      }\n    }\n\n    .leaf-ingredients__card::after{\n      content:\"\"; position:absolute; inset:0;\n      background: linear-gradient(180deg, rgba(255,255,255,0) 35%, rgba(255,255,255,.86) 68%, rgba(255,255,255,.98) 100%);\n      border-radius: inherit;\n      pointer-events:none;\n    }\n\n    .leaf-ingredients__card-body{\n      position:relative; z-index:2; text-align:center;\n      padding: 178px 15px 34px;\n    }\n    @media (max-width: 749px){\n      .leaf-ingredients__card-body{padding:150px 10px 30px;}\n    }\n\n    .leaf-ingredients__pill{\n      display:inline-block; background:#fff; border:1px solid var(--pill-bd);\n      color:var(--pill-text); font-weight:700; font-size:18px; line-height:1.44;\n      border-radius:9999px; padding:5px 11px; margin:0 auto 14px;\n      width:fit-content;\n    }\n    @media (max-width: 749px){ .leaf-ingredients__pill{font-size:16px; margin-bottom:12px;} }\n\n    .leaf-ingredients__text{\n      max-width: 262px; margin:0 auto 16px; color:var(--body-text);\n      letter-spacing:.01em; font-size:16px; line-height:1.45;\n    }\n    @media (max-