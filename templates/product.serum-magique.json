slidesPerView = window.innerWidth >= 1000 ? 3 : 1;\n    let currentPage = 0;\n    let pageCount = Math.ceil(totalSlides / slidesPerView);\n    let dots = [];\n\n    function getSlidesPerView() {\n      return window.innerWidth >= 1000 ? 3 : 1;\n    }\n\n    function buildDots() {\n      dotsContainer.innerHTML = '';\n      dots = [];\n      for (let i = 0; i < pageCount; i++) {\n        const dot = document.createElement('button');\n        if (i === currentPage) dot.classList.add('is-active');\n        dot.addEventListener('click', function () {\n          currentPage = i;\n          applyTransform();\n          updateDots();\n        });\n        dotsContainer.appendChild(dot);\n        dots.push(dot);\n      }\n    }\n\n    function updateDots() {\n      dots.forEach((dot, index) => {\n        dot.classList.toggle('is-active', index === currentPage);\n      });\n    }\n\n    function applyTransform() {\n      const slideWidth = slides[0].clientWidth;\n      const offset = slideWidth * slidesPerView * currentPage;\n      track.style.transform = `translateX(-${offset}px)`;\n    }\n\n    function recalc() {\n      slidesPerView = getSlidesPerView();\n      pageCount = Math.ceil(totalSlides / slidesPerView);\n      if (currentPage >= pageCount) currentPage = pageCount - 1;\n      if (currentPage < 0) currentPage = 0;\n      buildDots();\n      applyTransform();\n    }\n\n    btnPrev.addEventListener('click', function () {\n      if (currentPage > 0) {\n        currentPage--;\n        applyTransform();\n        updateDots();\n      }\n    });\n\n    btnNext.addEventListener('click', function () {\n      if (currentPage < pageCount - 1) {\n        currentPage++;\n        applyTransform();\n        updateDots();\n      }\n    });\n\n    window.addEventListener('resize', function () {\n      recalc();\n    });\n\n    // init\n    recalc();\n  })();\n</script>\n\n\n  \n</section>\n</div>",
