g = true;\n        requestAnimationFrame(()=>{\n          const left = grid.scrollLeft;\n          const slideW = slides[0].getBoundingClientRect().width;\n          const gap = parseInt(getComputedStyle(grid).columnGap || getComputedStyle(grid).gap) || parseInt(root.dataset.mobileGap) || 0;\n          const idx = Math.round(left / (slideW + gap));\n          setActive(Math.min(Math.max(idx,0), slides.length-1));\n          ticking = false;\n        });\n      }, { passive:true });\n    }\n\n    // Autoplay immÃ©diat de toutes les vidÃ©os (muted + playsinline)\n    const vids = slides.map(s => s.querySelector('video')).filter(Boolean);\n    vids.forEach(v => {\n      v.muted = true;\n      v.setAttribute('playsinline','');\n      if (v.play) { v.play().catch(()=>{}); } // ignore les rejets d'autoplay\n    });\n  })();\n  </script>\n</section>\n\n</div>",
