        "liquid_JppfTT": {
          "type": "liquid",
          "settings": {
            "liquid": "<div class=\"product-block\">\n  <style>\n    .dvry {\n      font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;\n      font-size: 14px;\n      margin-bottom: 16px;\n    }\n\n    /* Bloc principal */\n    .dvry__wrap {\n      background: #fff;\n      border: 2px solid #111;\n      border-radius: 14px;\n      padding: 18px;\n    }\n\n    /* Badges de confiance parents */\n    .dvry__trust-badges {\n      display: grid;\n      grid-template-columns: repeat(4, 1fr);\n      gap: 10px;\n      margin-bottom: 18px;\n    }\n\n    .dvry__trust-item {\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      gap: 8px;\n      padding: 14px 8px;\n      background: #fff;\n      border: 1.5px solid #111;\n      border-radius: 10px;\n      text-align: center;\n    }\n\n    .dvry__trust-icon {\n      width: 32px;\n      height: 32px;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n    }\n\n    .dvry__trust-icon svg {\n      width: 100%;\n      height: 100%;\n      stroke-width: 1.5;\n      stroke-linecap: round;\n      stroke-linejoin: round;\n    }\n\n    .dvry__trust-icon--ce {\n      font-size: 20px;\n      font-weight: 800;\n      color: #111;\n      letter-spacing: -0.5px;\n    }\n\n    .dvry__trust-text {\n      font-size: 11px;\n      font-weight: 600;\n      color: #111;\n      line-height: 1.3;\n      text-transform: uppercase;\n      letter-spacing: 0.2px;\n    }\n\n    /* Section dates */\n    .dvry__dates {\n      display: flex;\n      align-items: center;\n      justify-content: space-between;\n      gap: 12px;\n      padding: 16px;\n      margin-bottom: 14px;\n      background: #fff;\n      border: 1.5px solid #111;\n      border-radius: 10px;\n    }\n\n    .dvry__date-item {\n      flex: 1;\n    }\n\n    .dvry__date-label {\n      display: block;\n      font-size: 11px;\n      font-weight: 600;\n      color: #666;\n      text-transform: uppercase;\n    